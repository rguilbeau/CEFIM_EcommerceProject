<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="products" class="fr.romainguilbeau.cefim.ecommerce.services.ProductServiceImpl"/>

    <bean id="stocks" class="fr.romainguilbeau.cefim.ecommerce.services.StockServiceImpl"/>

    <bean id="orders" class="fr.romainguilbeau.cefim.ecommerce.services.OrderServiceImpl">
        <property name="stockService" ref="stocks"/>
    </bean>

</beans>